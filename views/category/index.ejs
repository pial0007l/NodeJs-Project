<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>
	<h2>Category List</h2>
	<a href="/home">Back</a> | <a href="/category/create">Create New</a>
	<br/><br/>
	<table border="1">
		<tr>
			<th>ID</th>
			<th>NAME</th>
			<th>DESC</th>
			<th>OPTION</th>
		</tr>
		<% for(var i = 0; i < catList.length; i++) {%>
		<tr>
			<td><%= catList[i].id %></td>
			<td><%= catList[i].name %></td>
			<td><%= catList[i].description %></td>
			<td><a href="/category/edit/<%= catList[i].id %>">Edit</a> | <a href="/category/delete/<%= catList[i].id %>">Delete</a></td>
		</tr>
		<% } %>
	</table>
</body>
</html>